   <select id="fotolist" parameterType="imgb" resultType="imgb">
   SELECT imgb_no,imgb_title,m_name,IMGB_THUMB,IMGB_CNT
        FROM(
        select ROWNUM as RNUM,imgb_no,imgb_title,m_name,IMGB_CNT,IMGB_THUMB
        from foto_view
        )WHERE RNUM between 1 and 12
   </select>
</mapper>
