<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.site.client.board.imgb.dao.ClImgbDao">
	<select id="clImgbList" parameterType="imgb" resultType="imgb">
		select imgb_title,imgb_content, imgb_img1, imgb_img2,imgb_img3,imgb_img4,imgb_img5,
		to_char(imgb_date, 'YYYY-MM-DD') as imgb_date
		from img_board
		order by imgb_no desc
	</select>
	
